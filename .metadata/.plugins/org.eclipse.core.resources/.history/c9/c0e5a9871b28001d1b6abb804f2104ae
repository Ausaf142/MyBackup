package BasicInerviewProg;

public class BasicTest {
	public static void main(String[] args) {
		int[] a= {7,5,6,9,15};
		
		for(int i=0;i<a.length;i++) {
			for(int j=1;j<a.length;j++) {
				if(a[i]>a[j]) {
					int temp=a[i];
					a[i]=a[j];
					a[j]=temp;
				}
			}
			System.out.println(a);
			}
		}
	}


